{
  "name": "explorer",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@monaco-editor/react": "^3.0.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@yllet/react": "^3.0.2",
    "autoprefixer": "10.4.2",
    "eslint-config-next": "12.1.0",
    "next": "^12.1.0",
    "postcss": "8.4.8",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint && tsc",
    "format": "prettier --single-quote --write 'src/**/*.ts*'",
    "deploy": "next build && next export && touch out/.nojekyll && REPO=$(git config --get remote.origin.url) && cd out && git init && git add . && git commit -m 'Deployed to Github Pages' && git push --force $REPO master:gh-pages && rimraf out"
  }
}
